               li.nav-item(id="pill-tab-1" data-bs-toggle="tab" data-bs-target="#pill-tab-1-content" role="tab") صنایع
               li.nav-item(id="pill-tab-2" data-bs-toggle="tab" data-bs-target="#pill-tab-2-content" role="tab") مفاهیم
               li.nav-item.active(id="pill-tab-3" data-bs-toggle="tab" data-bs-target="#pill-tab-3-content" role="tab") مقالات

            div.d-none.d-lg-flex(data-aos="fade-up" data-aos-duration="700")
               a(href="#").btn.btn-primary.btn-lg مشاهده همه

